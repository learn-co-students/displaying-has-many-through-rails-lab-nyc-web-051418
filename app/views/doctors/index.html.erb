<!-- Create a Doctor#index page that displays each doctor's name, a link to their show page, and a count of their patients. -->

<h1>Doctors Index</h1>
<ol>
  <% @doctors.each do |doctor| %>
    <li>
      <%= link_to "#{doctor.name}", doctor_path(doctor) %>
      <p>has <%=  pluralize(doctor.patients.count, "patient") %></p>
    </li>
    <br>
  <% end %>
</ol>
